# Prometheus Server Configuration
# Copy this file to .env and fill in your values

# Your Prometheus server URL
# Examples:
#   http://localhost:9090
#   http://prometheus.yourcompany.com:9090
#   https://prometheus.yourcompany.com
PROMETHEUS_URL=http://localhost:9090

# Basic Authentication (optional)
# Only required if your Prometheus server requires authentication
PROMETHEUS_USERNAME=
PROMETHEUS_PASSWORD=
